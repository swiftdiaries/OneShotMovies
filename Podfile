# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'OneShotMovies' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for OneShotMovies
	pod 'Alamofire', '~> 4.0'
    	pod "AsyncSwift"
    	pod 'SQLite.swift', '~> 0.11.2'


  target 'OneShotMoviesTests' do
    inherit! :search_paths
    # Pods for testing
	 pod 'Alamofire', '~> 4.0'
	 pod "AsyncSwift"
    	 pod 'SQLite.swift', '~> 0.11.2'

  end

  target 'OneShotMoviesUITests' do
    inherit! :search_paths
    # Pods for testing
	 pod 'Alamofire', '~> 4.0'
	    pod "AsyncSwift"
    	pod 'SQLite.swift', '~> 0.11.2'


  end

end
